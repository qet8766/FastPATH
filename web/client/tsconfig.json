{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "useDefineForClassFields": true,
    "types": ["vite/client", "@webgpu/types", "vitest/globals"],
    "noEmit": true
  },
  "include": ["src", "../tests/client"]
}
